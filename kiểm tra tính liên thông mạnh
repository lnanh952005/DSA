#include <bits/stdc++.h>
using namespace std;
vector <int> ke[1001];
vector < pair<int,int> > dscanh;
int used[1001];
int n,m,ok;
void DFS(int u){
	used[u]=1;
	for(int v : ke[u]){
		if(used[v]==0){
			used[v]=1;
			DFS(v);
		}
	}
}
void nhap(){
	cin >> n >> m;
	while(m--){
		int x,y; cin >> x >> y;
		dscanh.push_back({x,y});
		ke[x].push_back(y);
	}
}
int main() {
	int t; cin >> t;
	while(t--){
		nhap();
		ok=1;
		DFS(1);
		for(int i=1;i<=n;i++){
			if(used[i] == 0) ok=0;
		}
		if(ok) cout << "YES\n";
		else cout <<"NO\n";
		for(int i=1;i<=n;i++) ke[i].clear();
		dscanh.clear();
		memset(used,0,sizeof(used));
	}
}
